import java.util.*;
public class GradeCalculator {
	public static void grade(int number,int marks) {
		int avg= marks/number ;
		System.out.println("Average percentage:"+avg);
		if(avg>=90) {
			System.out.println("Your Grade:O");	
		}
		else if(avg>=80) {
			System.out.println("Your Grade:A");
		}
		else if(avg>=70) {
			System.out.println("Your Grade:B");
		}
		else if(avg>=60) {
			System.out.println("Your Grade:C");
		}
		else if(avg>=50) {
			System.out.println("Your Grade in:D");
		}
		else if(avg>=40) {
			System.out.println("Your Grade:Pass");
		}
		else {
			System.out.println("Your Grade:Fail");
		}
	}
	public static void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		System.out.print("Enter the number of subjects");
		int n=sc.nextInt();
		int t_marks=0;
		for(int i=0;i<n;i++) {
			System.out.println("Enter subject marks");
			int m=sc.nextInt();
			t_marks+=m;
		}
		System.out.println("Total marks:"+t_marks);
		grade(n,t_marks);
		sc.close();
		
		}

}
